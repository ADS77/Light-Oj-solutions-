#include<bits/stdc++.h>
using namespace std;
const int inf = 0x3f3f3f3f;
#define ll long long
#define mod 998244353
#define pb push_back
#define mk make_pair
#define  ull unsigned long long
#define fast ios_base::sync_with_stdio(false); cin.tie(0);cout.tie(0);
int main()
{
   fast ;
   ll t;
   cin>>t;
ll x1,y1,x2,y2;
   for(int i=1;i<=t;i++)
   {
       cin>>x1>>y1>>x2>>y2;
       cout<<"Case "<<i<<":"<<endl;
       ll p;
       cin>>p;
       ll  m,n;
       while(p--)
       {
           cin>>m>>n;
           if(m>=x1 && m<=x2 && n>=y1 && n<=y2)
            cout<<"Yes"<<endl;
           else cout<<"No"<<endl;
 
       }
   }
}
 
